### Starting build.
# Target: main.ml.depends, tags: { extension:ml, file:main.ml, ocaml, ocamldep, package(io-system), quiet }
ocamlfind ocamldep -package io-system -modules main.ml > main.ml.depends
# Target: main.mli.depends, tags: { extension:mli, file:main.mli, ocaml, ocamldep, package(io-system), quiet }
ocamlfind ocamldep -package io-system -modules main.mli > main.mli.depends
# Target: main.cmi, tags: { byte, compile, extension:mli, file:main.mli, interf, ocaml, package(io-system), quiet }
ocamlfind ocamlc -c -package io-system -o main.cmi main.mli
# Target: main.cmx, tags: { compile, extension:cmx, extension:ml, file:main.cmx, file:main.ml, implem, native, ocaml, package(io-system), quiet }
ocamlfind ocamlopt -c -package io-system -o main.cmx main.ml
# Target: main.native, tags: { dont_link_with, extension:native, file:main.native, link, native, ocaml, package(io-system), program, quiet }
ocamlfind ocamlopt -linkpkg -package io-system main.cmx -o main.native
# Compilation successful.
